faset_target_meta <- function(f, Target.name) {
t <- f$table
st <- t %>% filter(Target==Target.name)
info <- list()
info$top3 <- st %>% top_n(3, n)
info$setsize <- length(st$Assoc)
info$total <- sum(st$n)
return(info)
}
